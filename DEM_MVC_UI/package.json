{
  "name": "deus-ex-machina",
  "version": "0.0.1-alpha.1",
  "description": "UI part of DeusExMachina website",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/AndreyShpilevoy/DemProject"
  },
  "scripts": {
    "setEnv": "npm run setDevEnv && npm install -g npm-check-updates",
    "setDevEnv": "npm config set production false && npm config set dev true",
    "setProdEnv": "npm config set production true && npm config set dev false",
    "clean-wwwroot": "node_modules/.bin/rimraf ../DEM_MVC/wwwroot && mkdirp ../DEM_MVC/wwwroot",
    "lint": "node_modules/.bin/eslint --color webpack.config.* src tools",
    "test": "cross-env BABEL_ENV=test jest --no-cache",
    "scssLint": "node_modules/.bin/sass-lint -v -q",
    "prebuild": "npm-run-all clean-wwwroot lint scssLint test",
    "build": "cross-env BABEL_ENV=production babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "start": "yarn run scssLint && yarn run test && cross-env BABEL_ENV=development babel-node tools/srcServer.js"
  },
  "jest": {
    "transform": {
      ".css": "<rootDir>/node_modules/jest-css-modules",
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!dem-ui-grid)"
    ],
    "testRegex": "(.*/__tests__/.*|.*/test.js)",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "(.*/__tests__/.*|.*/(test.js|index.scss))",
      "<rootDir>/testHelpers/",
      "<rootDir>/src/scripts/services/domScripts/SelectTextFromContainerById.js",
      "<rootDir>/src/scripts/services/domScripts/ShrinkingHeader.js",
      "<rootDir>/src/scripts/services/domScripts/ToggleClass.js",
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-plugin-transform-react-router-optimize": "1.0.1",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "checksum": "0.1.1",
    "colors": "1.1.2",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "cssnano": "3.10.0",
    "enzyme": "2.7.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.9.0",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "jest-cli": "18.1.0",
    "jest-css-modules": "1.1.0",
    "jsdom": "9.10.0",
    "mkdirp": "0.5.1",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.1",
    "open": "0.0.5",
    "postcss-initial": "1.5.3",
    "postcss-loader": "1.2.2",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.2.2",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.2",
    "sass-loader": "5.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-hot-middleware": "2.16.1"
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "dem-ui-grid": "git://github.com/AndreyShpilevoy/DemUiGrid.git",
    "lodash": "4.17.4",
    "pace-progress": "1.0.2",
    "raven-js": "3.10.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-notification-system": "0.2.11",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-saga": "0.14.3"
  }
}
