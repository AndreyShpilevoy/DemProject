{
  "name": "deus-ex-machina",
  "version": "1.0.0",
  "description": "UI part of DeusExMachina website",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/AndreyShpilevoy/DemProject"
  },
  "scripts": {
    "f": "npm i -g yarn && yarn",
    "clean-wwwroot": "node_modules/.bin/rimraf ../DEM_MVC/wwwroot && mkdirp ../DEM_MVC/wwwroot",
    "lint": "node_modules/.bin/eslint --color webpack.config.* src tools",
    "test": "cross-env BABEL_ENV=test jest --no-cache",
    "prebuild": "npm-run-all clean-wwwroot lint test",
    "build": "cross-env BABEL_ENV=production babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "start": "yarn test && cross-env BABEL_ENV=development babel-node tools/srcServer.js"
  },
  "jest": {
    "transform": {
      ".css": "<rootDir>/node_modules/jest-css-modules",
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(.*/__tests__/.*|\\.(test|spec))\\.js|.jsx",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverage": true,
    "testPathIgnorePatterns":[
      "<rootDir>/node_modules/",
      "<rootDir>/tempTestFolder/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/testHelpers/",
      "<rootDir>/src/scripts/services/domScripts/SelectTextFromContainerById.js",
      "<rootDir>/src/scripts/services/domScripts/ShrinkingHeader.js",
      "<rootDir>/src/scripts/services/domScripts/ToggleClass.js",
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.5.3",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.7",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-module-resolver": "2.3.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-plugin-transform-react-router-optimize": "1.0.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "bootstrap-loader": "2.0.0-beta.14",
    "checksum": "0.1.1",
    "colors": "1.1.2",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "cssnano": "3.8.0",
    "enzyme": "2.6.0",
    "eslint": "3.10.2",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-babel-module": "2.2.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.7.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "jest-cli": "17.0.2",
    "jest-css-modules": "1.1.0",
    "jsdom": "9.8.3",
    "mkdirp": "0.5.1",
    "node-sass": "3.13.0",
    "npm-run-all": "3.1.1",
    "open": "0.0.5",
    "postcss-initial": "1.5.2",
    "postcss-loader": "1.1.1",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.2.1",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "bootstrap": "4.0.0-alpha.4",
    "lodash": "4.17.2",
    "pace-progress": "1.0.2",
    "raven-js": "3.8.1",
    "react": "15.3.2",
    "react-cookie": "0.4.8",
    "react-dom": "15.3.2",
    "react-notification-system": "0.2.10",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-saga": "0.13.0"
  }
}
