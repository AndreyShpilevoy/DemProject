{
  "name": "deus-ex-machina",
  "version": "1.0.0",
  "description": "UI part of DeusExMachina website",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/AndreyShpilevoy/DemProject"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint --color webpack.config.* src tools",
    "install:all": "npm install --only=dev && npm install --only=prod && npm run generateMapFiles",
    "prestart": "npm run generateMapFiles",
    "start": "npm run test && babel-node tools/srcServer.js",
    "clean-wwwroot": "node_modules/.bin/rimraf ../DEM_MVC/wwwroot && mkdirp ../DEM_MVC/wwwroot",
    "g": "npm run generateMapFiles",
    "generateMapFiles": "babel-node tools/mapFilesGenerator.js",
    "prebuild": "npm-run-all clean-wwwroot generateMapFiles lint test",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRegex": "(.*/__tests__/.*|\\.(test|spec))\\.js|.jsx",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/testHelpers/",
      "<rootDir>/src/scripts/utils/ShrinkingHeader.js",
      "<rootDir>/src/scripts/utils/ToggleClass.js",
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-plugin-transform-react-router-optimize": "1.0.1",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.14.0",
    "bootstrap-loader": "2.0.0-beta.11",
    "checksum": "0.1.1",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "cssnano": "3.7.5",
    "enzyme": "2.4.1",
    "eslint": "3.6.0",
    "eslint-config-airbnb-base": "8.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-react": "6.3.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "jest-cli": "15.1.1",
    "jsdom": "9.5.0",
    "node-sass": "3.10.0",
    "npm-run-all": "3.1.0",
    "open": "0.0.5",
    "path": "0.12.7",
    "postcss-loader": "0.13.0",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "3.0.0-beta.5",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "^1.2.1",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "1.8.2",
    "webpack-hot-middleware": "2.12.2"
  },
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "bootstrap": "4.0.0-alpha.4",
    "lodash": "4.16.1",
    "pace-progress": "1.0.2",
    "react": "15.3.2",
    "react-cookie": "0.4.8",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-saga": "0.11.1"
  }
}
