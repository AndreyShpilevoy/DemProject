{
  "name": "deus-ex-machina",
  "version": "1.0.0",
  "description": "UI part of DeusExMachina website",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/AndreyShpilevoy/DemProject"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint --color webpack.config.* src tools",
    "install:all": "npm install --only=dev && npm install --only=prod",
    "ia": "npm run install:all",
    "start": "npm run test && babel-node tools/srcServer.js",
    "clean-wwwroot": "node_modules/.bin/rimraf ../DEM_MVC/wwwroot && mkdirp ../DEM_MVC/wwwroot",
    "prebuild": "npm-run-all clean-wwwroot lint test",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRegex": "(.*/__tests__/.*|\\.(test|spec))\\.js|.jsx",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/testHelpers/",
      "<rootDir>/src/scripts/utils/ShrinkingHeader.js",
      "<rootDir>/src/scripts/utils/ToggleClass.js",
      "<rootDir>/node_modules/"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-module-resolver": "2.2.0",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-plugin-transform-react-router-optimize": "1.0.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.16.3",
    "bootstrap-loader": "2.0.0-beta.12",
    "checksum": "0.1.1",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "cssnano": "3.8.0",
    "enzyme": "2.5.1",
    "eslint": "3.8.1",
    "eslint-config-airbnb-base": "9.0.0",
    "eslint-import-resolver-babel-module": "2.0.1",
    "eslint-loader": "1.6.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-react": "6.4.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.0",
    "jest-cli": "16.0.2",
    "jsdom": "9.8.0",
    "node-sass": "3.10.1",
    "npm-run-all": "3.1.1",
    "open": "0.0.5",
    "path": "0.12.7",
    "postcss-loader": "1.0.0",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.2.1",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "bootstrap": "4.0.0-alpha.4",
    "lodash": "4.16.4",
    "pace-progress": "1.0.2",
    "raven-js": "3.7.0",
    "react": "15.3.2",
    "react-cookie": "0.4.8",
    "react-dom": "15.3.2",
    "react-notification-system": "0.2.10",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-saga": "0.12.0"
  }
}
