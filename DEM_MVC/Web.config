<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <connectionStrings>
    <add name="DemConnectionString" providerName="System.Data.SqlClient" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=DEM_Project; User ID=DemUser; Password=4252744;" />
  </connectionStrings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target xsi:type="Database" name="dbTarget" connectionStringName="DemConnectionString" commandText="exec Log_Insert                                  @LogDate,                                 @CallSite,                                 @ExceptionType,                                 @ExceptionMessage,                                 @StackTrace,                                 @InnerExeption,                                 @Message,                                 @LogLevel                                 ">
        <parameter name="@LogDate" layout="${date}" />
        <parameter name="@CallSite" layout="${callsite}" />
        <parameter name="@ExceptionType" layout="${exception:format=Type}" />
        <parameter name="@ExceptionMessage" layout="${exception:format=Message}" />
        <parameter name="@StackTrace" layout="${exception:format=StackTrace}" />
        <parameter name="@InnerExeption" layout="${exception:method:maxInnerExceptionLevel=5:innerFormat=shortType,message,method}" />
        <parameter name="@Message" layout="${message}" />
        <parameter name="@LogLevel" layout="${level:uppercase=true}" />
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="Trace" writeTo="dbTarget" />
    </rules>
  </nlog>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <customErrors mode="On">
      <error statusCode="401" redirect="~/Errors/401.shtml" />
      <error statusCode="402" redirect="~/Errors/402.shtml" />
      <error statusCode="403" redirect="~/Errors/403.shtml" />
      <error statusCode="404" redirect="~/Errors/404.shtml" />
      <error statusCode="500" redirect="~/Errors/500.shtml" />
    </customErrors>
  </system.web>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" />
  </system.webServer>
