<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	<link type="text/css" rel="stylesheet" href="~/wwwroot/dist/styles/dem.min.css" />
</head>
<body>
	<div id="wrap">
		<div id="header" class="container navbar-fixed-top">
			<div class="row">
				<div class="col-xs-12">
					<nav class="navbar navbar-dark navbar-full bg-dem-brown-sm padding-initial navbar-dem-logo-bg navbar-height">
						<div class="navbar-header navbar-header-add-navbar-padding col-xs-12 col-lg-6">
							<!-- Logo -->
							<a class="navbar-brand navbar-brand-size padding-initial" href="#">
								<img src="../../wwwroot/dist/images/DEM/Dem_Logo.png" alt="DeusExMachina logo" />
							</a>
							<!-- Toggle Button -->
							<button type="button" class="navbar-toggler hidden-lg-up pull-xs-right" id="menu-toggle-button" data-toggle="collapse" data-target="#nav-menu-content">
								<span class="sr-only">Toggle navigation</span>
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
								<span class="icon-bar"></span>
							</button>
						</div>
						<div class="col-xs-12 col-lg-6 menu-transform">
							<!-- Links -->
							<ul class="nav navbar-nav collapse navbar-toggleable-md" id="nav-menu-content">
								<li class="nav-item">
									<a class="nav-link" href="#">Link 1</a>
									<div class="nav-link-separator"></div>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="#">Link 2</a>
									<div class="nav-link-separator"></div>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="#">Link 3</a>
									<div class="nav-link-separator"></div>
								</li>
							</ul>
						</div>
					</nav>
				</div>
			</div>
		</div>

		<div id="pageContent">
			@RenderBody()
		</div>
	</div>

	<div id="footer" class="container">
		<div class="row">
			<div class="col-xs-12">
				<nav class="navbar padding-initial footer-style">
					<div></div>
				</nav>
			</div>
		</div>
	</div>
	<script src="~/wwwroot/dist/scripts/dem.min.js"></script>
</body>
</html>
