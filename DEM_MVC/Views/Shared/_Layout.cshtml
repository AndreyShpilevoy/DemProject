<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	<link type="text/css" rel="stylesheet" href="~/wwwroot/dist/styles/dem.min.css" />
</head>
<body>
	<div id="header" class="container">
		<div class="row">
			<div class="col-xs-12">
				<nav class="navbar navbar-dark bg-dem-brown-sm">
					<div class="navbar-header col-xs-12 col-lg-6">
						<!-- Logo -->
						<a class="navbar-brand padding-initial" href="#">
							<img src="../../wwwroot/dist/images/DEM/Dem_Logo_SM.png" alt="DeusExMachina logo small" />
						</a>
						<!-- Toggle Button -->
						<button type="button" class="navbar-toggler hidden-lg-up pull-xs-right" data-toggle="collapse" data-target="#nav-content">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
					</div>
					<div class="col-xs-12 col-lg-6">
						<!-- Links -->
						<ul class="nav navbar-nav collapse navbar-toggleable-md" id="nav-content">
							<li class="nav-item">
								<a class="nav-link" href="#">Link 1</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="#">Link 2</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="#">Link 3</a>
							</li>
						</ul>
					</div>
				</nav>
			</div>
		</div>
	</div>

	@RenderBody()
	<script src="~/wwwroot/dist/scripts/dem.min.js"></script>
</body>
</html>
